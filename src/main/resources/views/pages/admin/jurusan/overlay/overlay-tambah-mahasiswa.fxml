        <TextField fx:id="inputNama" prefHeight="26.0" prefWidth="502.0" />

        <Label text="NIM" />
        <TextField fx:id="inputNim" prefHeight="26.0" prefWidth="502.0" />

        <Label text="Jurusan" />
        <TextField fx:id="inputJurusan" prefHeight="26.0" prefWidth="502.0" />

        <Label text="Prodi" />
      <ChoiceBox fx:id="inputProdi" maxWidth="1.7976931348623157E308" />

        <Label text="Email" />
        <TextField fx:id="inputEmail" prefHeight="26.0" prefWidth="502.0" />

        <Label text="Password" />
        <PasswordField fx:id="inputPassword" prefHeight="26.0" prefWidth="502.0" />
      <Label layoutX="40.0" layoutY="256.0" text="Konfirmasi Password" />
      <PasswordField fx:id="inputKonfirmasiPassword" layoutX="40.0" layoutY="274.0" prefHeight="26.0" prefWidth="502.0" />

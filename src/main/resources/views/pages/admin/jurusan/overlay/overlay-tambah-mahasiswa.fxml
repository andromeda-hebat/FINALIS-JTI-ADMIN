            <Font name="Reddit Sans Regular" size="12.0" />
         </font>
      </Button>

        <HBox alignment="CENTER" prefHeight="77.0" prefWidth="430.0">
            <children>
                <Button fx:id="btnTambahkan" alignment="BOTTOM_CENTER" mnemonicParsing="false" onAction="#submitForm" style="-fx-background-color: #052C65;" text="Tambahkan" textFill="WHITE" />
            </children>
         <VBox.margin>
            <Insets bottom="30.0" top="30.0" />
         </VBox.margin>
        </HBox>
    </children>
    <padding>
        <Insets bottom="20.0" left="30.0" right="40.0" top="30.0" />
    </padding>

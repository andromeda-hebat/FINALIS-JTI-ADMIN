                     <Label text="Seluruh Permintaan">
                        <font>
                           <Font name="System Bold" size="14.0" />
                        </font></Label>
                  </children>
               </VBox>
               <HBox nodeOrientation="RIGHT_TO_LEFT" prefHeight="20.0" prefWidth="352.0" HBox.hgrow="ALWAYS">
                  <children>
                     <Label text="Sort">
                        <font>
                           <Font name="System Bold" size="14.0" />
                        </font></Label>
                     <ImageView fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true" HBox.hgrow="ALWAYS">

        <children>
            <fx:include source="../../components/admin/topbar.fxml" />
            <HBox alignment="CENTER_LEFT" prefHeight="50.0" prefWidth="200.0">
                <children>
                    <ImageView fitHeight="30.0" fitWidth="30.0" pickOnBounds="true" preserveRatio="true">
                        <image>
                            <Image url="@../../img/arrow-left-square.png" />
                        </image>
                        <HBox.margin>
                            <Insets right="20.0" />
                        </HBox.margin>
                    </ImageView>
                    <Label fx:id="judulHalaman" text="Permintaan Verifikasi Tugas Akhir">
                        <font>
                            <Font name="Reddit Sans Black" size="24.0" />
                        </font>
                    </Label>
                </children>
                <VBox.margin>
                    <Insets left="20.0" right="20.0" top="30.0" />
                </VBox.margin>
            </HBox>
         <HBox prefHeight="19.0" prefWidth="560.0">
            <VBox.margin>

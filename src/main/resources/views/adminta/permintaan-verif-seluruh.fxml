                    <TableColumn fx:id="nameColumn" editable="false" prefWidth="136.00003051757812" text="Mahasiswa" />
                    <TableColumn fx:id="descriptionColumn" editable="false" prefWidth="168.800048828125" sortable="false" text="Keterangan" />
                    <TableColumn fx:id="dateColumn" editable="false" prefWidth="96.0" text="Tanggal" />
                    <TableColumn fx:id="actionColumn" editable="false" prefWidth="64.0" sortable="false" text="Aksi" />
                </columns>
                <opaqueInsets>
                    <Insets />

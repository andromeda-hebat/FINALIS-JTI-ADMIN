                     </ImageView>
                  </children>
               </HBox>
            </children>
         </HBox>
            <TableView fx:id="tableViewBerkasTA" VBox.vgrow="ALWAYS">
                <columns>
               <TableColumn fx:id="numberColumn" editable="false" prefWidth="75.0" text="No" />
